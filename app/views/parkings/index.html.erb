<p id="notice"><%= notice %></p>

<h1>Parkings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Typedocument</th>
      <th>Document</th>
      <th>Address</th>
      <th>Userid</th>
      <th>Phone</th>
      <th>Movil</th>
      <th>Email</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Servicedays</th>
      <th>Openingtime</th>
      <th>Closingtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parkings.each do |parking| %>
      <tr>
        <td><%= parking.name %></td>
        <td><%= parking.typeDocument %></td>
        <td><%= parking.document %></td>
        <td><%= parking.address %></td>
        <td><%= parking.userId %></td>
        <td><%= parking.phone %></td>
        <td><%= parking.movil %></td>
        <td><%= parking.email %></td>
        <td><%= parking.latitude %></td>
        <td><%= parking.longitude %></td>
        <td><%= parking.serviceDays %></td>
        <td><%= parking.openingTime %></td>
        <td><%= parking.closingTime %></td>
        <td><%= link_to 'Show', parking %></td>
        <td><%= link_to 'Edit', edit_parking_path(parking) %></td>
        <td><%= link_to 'Destroy', parking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parking', new_parking_path %>
